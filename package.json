  "private": true,
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "12.12.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "socket.io-client": "^4.3.2",
    "sweetalert2": "^11.1.9",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "serve": "serve -s build",
    "load-env": "node export-env.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preelectron-pack": "yarn load-env && yarn build:electron && yarn build",
    "build:electron": "tsc --project tsconfig.electron.json",
    "electron-pack": "yarn build:electron && electron-builder -c.extraMetadata.main=build/electron.js",
    "electron-dev": "yarn load-env && yarn build:electron && concurrently  \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/electron-is-dev": "^1.1.1",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.1",
    "@types/redux": "^3.6.0",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "electron": "9.2.0",
    "electron-builder": "^22.13.1",
    "serve": "^13.0.2",
    "server": "^1.0.36",
    "wait-on": "^6.0.0"
  },
  "build": {
    "appId": "com.jacobo.cmu",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
